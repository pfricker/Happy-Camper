<div class="row">
  <div class="medium-12 medium-centered columns">
    <h1>AVAILABLE BACKPACKS</h1>
  </div>
</div>

<div class="row">
  <div class="medium-4 columns float-left" id="box">
    <ul>
      <% @backpacks.each do |backpack| %>
        <li> <%= link_to backpack.name, backpack %> : <%= backpack.user.username %></li>
      <% end %>
    </ul>
  </div>
  <div class="medium-6 columns float right"id="map_wrapper">
      <div id="map_canvas" class="mapping"></div>
  </div>
</div>
<br>
<div class="row">
  <div class="medium-4 text-center columns" id="box">
    <h6><%= paginate @backpacks %></h6>
  </div>
</div>
