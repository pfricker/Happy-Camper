<div class="row">
  <div class="medium-12 medium-centered columns text centered">
    <h1> Available Tents</h1>
  </div>
</div>

<div class="row">
  <div class="medium-12 medium-centered columns">
    <table>
      <tbody>
        <% @tents.each do |tent| %>
        <tr>
          <td> <%= link_to tent.name, tent %> </td>
          <td> <%= tent.user.username %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
